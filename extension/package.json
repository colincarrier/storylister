{
  "name": "storylister-extension",
  "version": "1.0.0",
  "description": "Instagram Story Viewer Enhancement Chrome Extension",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config build/webpack.config.js",
    "build:dev": "webpack --config build/webpack.config.js --mode development",
    "watch": "webpack --config build/webpack.config.js --watch",
    "clean": "rm -rf dist",
    "package": "npm run build && cd dist && zip -r ../storylister-extension.zip .",
    "lint": "eslint . --ext .ts,.js",
    "type-check": "tsc --noEmit --project build/tsconfig.json"
  },
  "keywords": [
    "instagram",
    "chrome-extension",
    "story-viewer",
    "analytics"
  ],
  "author": "Storylister Team",
  "license": "MIT",
  "devDependencies": {
    "@types/chrome": "^0.0.246",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "^8.0.0",
    "ts-loader": "^9.4.0",
    "typescript": "^5.0.0",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.0"
  },
  "dependencies": {}
}
