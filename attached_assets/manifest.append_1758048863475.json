{
  "content_scripts": [
    {
      "matches": [
        "https://www.instagram.com/*"
      ],
      "run_at": "document_start",
      "js": [
        "content-backend.js",
        "content.js"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "injected.js"
      ],
      "matches": [
        "https://www.instagram.com/*"
      ]
    }
  ]
}